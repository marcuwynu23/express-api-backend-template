{
  "name": "express-api-backend-template",
  "version": "0.0.0",
  "private": true,
  "bin": "app.js",
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "build": "webpack --config webpack.config.js"
