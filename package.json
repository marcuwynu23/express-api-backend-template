    "express-session": "^1.17.3",
    "helmet": "^6.1.5",
    "http-errors": "~1.6.3",
    "morgan": "^1.10.0",
    "mysql2": "^3.1.2",
    "pg": "^8.9.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.0",
    "sqlite3": "^5.1.4",
    "tedious": "^15.1.3"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.21.4",
